{
  "manifest_version": 3,
  "name": "BobRossRocks",
  "version": "1.3.0",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*", "https://x.com/*"],
      "js": ["interceptor.js"],
      "run_at": "document_start",
      "world": "MAIN"
    },
    {
      "matches": ["https://twitter.com/*", "https://x.com/*"],
      "js": ["Content.js"],
      "run_at": "document_idle"
    }
  ]
}
